        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement controls 
//- todo the water isn't infinite in this

if (keyboard_check(ord("W")))
{
    motion_add(dir, playerAcceleration);
}

if (keyboard_check(ord("A")))
{
    motion_add(dir + 90, playerAcceleration);
}

if (keyboard_check(ord("S")))
{
    motion_add(dir + 180, playerAcceleration);
}

if (keyboard_check(ord("D")))
{
    motion_add(dir + 270, playerAcceleration);
}

//update direction
motion_add(dir, 0.00000000001);

//speed control
speed = clamp(speed,0,1.5);

//get the mouse movement in between steps and move the view accordingly
var xdif,ydif;
xdif = (window_mouse_get_x() - wd);
ydif = (window_mouse_get_y() - hd);
window_mouse_set(wd,hd);
dir = (dir - xdif / 4) mod 360;
zdir = clamp(zdir-(ydif / 4),-85,85);
    
//set look position vars
lookz = lengthdir_y(1,-zdir);
lookx = lengthdir_x(1-(abs(lookz)),dir);
looky = lengthdir_y(1-(abs(lookz)),dir);

//jump code
if keyboard_check_pressed(vk_space) &amp;&amp; jump == false 
{
    jump = true 
    jumpHeightModifier += 5.0
}

if jump == true 
{
    z += jumpHeightModifier
    jumpHeightModifier -= 0.5
    
    if z &lt;= player_height 
    {
        z = player_height 
        jumpHeightModifier = 0 
        jump = false
    }
}

//Raycast for block aiming
scr_Raycast();

//Collision controller
scr_CollisionHandler();
</string>
          </argument>
