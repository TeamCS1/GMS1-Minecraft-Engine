            <kind>1</kind>
            <string>//set up basic control vars for the camera
z = 80;
dir = 0;
zdir = 0;
speed = 0;
friction = .1;
lookx = 0;
looky = 0;
lookz = 0;
player_height = 80;
playerAcceleration = 0.50;
jump = false; 
jumpHeightModifier = 0;
raycastDelay = 0;
directionText = "";

//extra vars for detecting mouse movement
wd = window_get_width()/2;
hd = window_get_height()/2;
window_mouse_set(wd,hd);

//start 3d mode
d3d_start();
d3d_set_lighting(false);
d3d_set_culling(true);
d3d_set_shading(true);
d3d_set_fog(true,c_skyfog,global.fog_near,global.fog_far);
d3d_light_define_direction(1,.5,.25,-1,c_gray);
d3d_light_enable(1,true);
</string>
