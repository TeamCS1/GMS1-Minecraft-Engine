            <kind>1</kind>
            <string>//we need to do a few things to prepare to draw in 2d, onto the surface

//disable lighting and fog
d3d_set_lighting(false);
d3d_fog_enable(false);

//disable perspective drawing and set the projection to orthographic
d3d_set_perspective(false);
d3d_set_projection_ortho(0,0,960,540,0);

//disable culling - for some reason culling prevents images from being drawn here
d3d_set_culling(false);

//check to make sure the water surface exists, and if not, recreate it
if (!surface_exists(global.water_surface)){
    global.water_surface = surface_create(256,256);
    }

//begin drawing to the water surface 
surface_set_target(global.water_surface);
    
    //reset drawing
    draw_reset();
    
    //clear the surface with a dark gray, to set the stage for the rest of the effect
    draw_clear(c_dkgray);
    
    //use this custom blend mode to overlay a bunch of layers of noise
    draw_set_blend_mode_ext(bm_dest_color,bm_dest_color);
    
    //draw the noise layers
    draw_background_tiled_ext(bg_noise_16,water_offset,water_offset,16,16,c_white,.125);
    draw_background_tiled_ext(bg_noise_32,water_offset*2,water_offset,8,8,c_white,.125);
    draw_background_tiled_ext(bg_noise_64,water_offset*2,water_offset*2,4,4,c_white,.125);
    draw_background_tiled_ext(bg_noise_128,water_offset*4,water_offset,2,2,c_white,.125);
    draw_background_tiled_ext(bg_noise_4,water_offset*2,water_offset*4,64,64,c_white,.125);
    draw_background_tiled_ext(bg_noise_16,water_offset*4,water_offset*4,16,16,c_white,.125);
    draw_background_tiled_ext(bg_noise_8,water_offset,water_offset*2,32,32,c_white,.125);
    
    //reset the blend mode
    draw_set_blend_mode(bm_normal);
    
//stop drawing to the surface
surface_reset_target();

//not going to reset anything here since the camera and skybox will reset those settings anyway
</string>
